                            <textbox id="ModRec_tb" context="clipboard" class="plain" name="ModRec" maxlength="1" size="1" onkeypress="set_lock_on_keypress(event);" onblur="updateFixedFields(this);" onfocus="this.select();"/>
                            <label name="Source" control="Source_tb" set="AUT" value="Source" />
