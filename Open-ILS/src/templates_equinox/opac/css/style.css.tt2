    display: inline-block;
    letter-spacing: .0625em;
    line-height: 1;
    margin: 0 .3em;
    padding: .6em .8em;
    position: relative;
    text-transform: uppercase;
    transition: all .3s ease-out;
    vertical-align: middle;
}

a.opac-button:hover,
a:link.opac-button:hover,
a:visited.opac-button:hover,
a:active.opac-button:hover,
