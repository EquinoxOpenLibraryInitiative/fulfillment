    width: 50%;
}

a,
a:link,
a:visited,
a:active {
    color: #536090;
    cursor: pointer;
    text-decoration: none;
    transition: all .2s ease;
}

a:hover {
    color: #6AB5F9;
    cursor: pointer;
}

a.opac-button,
a:link.opac-button,
a:visited.opac-button,
a:active.opac-button,
#account-update-email input[type='submit'] {
    background: #38558A;
    border: 0;
    color: #fff;
