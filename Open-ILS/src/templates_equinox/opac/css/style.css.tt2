/* Image slider */

#carousel_title {
    font-size: 110%;
    display: block;
    text-align: center;
}

.tns-nav {
    text-align: center;
    margin: 10px 0;
}

.tns-nav > [aria-controls] {
    width: 20px;
    height: 12px;
    padding: 0;
    margin: 0 5px;
    border-radius: 20px;
    background: #999;
    border: 0;
}
.tns-nav > [aria-controls]:hover {
    background-color: #9ECC4C;
}

.tns-nav-active > [aria-selected="true"] {
    background-color: #6AB5F9;
}

